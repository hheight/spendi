{
  "name": "spendi-expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test:unit": "vitest run --config ./vitest.config.unit.ts",
    "test:int": "./scripts/run-integration.sh",
    "test:e2e": "./scripts/run-e2e.sh",
    "test:e2e:headed": "./scripts/run-e2e.sh --headed",
    "prepare": "husky",
    "docker:dev:start": "docker compose -f compose.yaml -f compose.dev.yaml up -d",
    "docker:dev:stop": "docker compose -f compose.yaml -f compose.dev.yaml down",
    "docker:test:stop": "docker compose -f compose.yaml -f compose.test.yaml down",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "db:dev:seed": "dotenv -e .env.development -- npx prisma db seed"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@prisma/client": "6.19.0",
    "@prisma/extension-accelerate": "2.0.2",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "bcryptjs": "3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "date-fns": "^4.1.0",
    "jose": "6.1.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.4",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-day-picker": "^9.11.2",
    "react-dom": "19.1.0",
    "react-hook-form": "7.65.0",
    "recharts": "^2.15.4",
    "server-only": "0.0.1",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@faker-js/faker": "10.1.0",
    "@playwright/test": "1.56.0",
    "@tailwindcss/postcss": "4",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@types/supertest": "6.0.3",
    "dotenv": "17.2.3",
    "eslint": "9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-playwright": "^2.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "prisma": "6.19.0",
    "supertest": "7.1.4",
    "tailwindcss": "4",
    "tsx": "4.20.6",
    "tw-animate-css": "1.4.0",
    "typescript": "5",
    "vitest": "3.2.4",
    "vitest-mock-extended": "3.1.0"
  }
}
